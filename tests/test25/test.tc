from std.io import println
from std.string import String

type Z<T> = class {
    let x: T

    fn init(x: T) {
        this.x = x
    }

    fn add(y: T) -> (T, T) {
        return (this.x, this.x+y)
    }

    static fn callme(x: u32, y: u32) -> (u32, u32) {
        return (x+1, y+5)
    }

    static fn callme2<T>(x: T, y: T) -> (T, T) {
        return (x+2, y+3)
    }

    static fn haha() = (1, "oupsy")
}

fn main() {


    let arr: u32[] = [1, 2, 3]
    let [x, y,...rest] = arr

    println("X = "+x)
    println("Y = "+y)
    println("Rest = "+rest.length+"!")

    for let i: u64 = 0; i < rest.length; i+=1 {
        println("Rest["+i+"] = "+rest[i])
    }

    let user = {name: "John", age: 30, is_student: true}

    let {name, ...res} = user

    println("Name: "+name)
    println("Age: "+res.age)
    println("Is student: "+res.is_student)

    let (aa, bb) = Z.haha()

    println("aa = "+aa)
    println("bb = "+bb)
}