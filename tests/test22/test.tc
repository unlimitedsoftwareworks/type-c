
let f = fn(x: u64[]) -> (u64, bool) {
    let i: u64 = 0
    while true {
        return (x[i], i++ == x.length)

        if i == x.length {
            i = 0
        }
    }
}

let g = coroutine(f, [1, 2, 3, 4])

let loop = true
while loop {
    let (x, done) = g()
    //io.println(x)
    if done {
        loop = false
    }
}