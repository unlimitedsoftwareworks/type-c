from std.io import println
from std.String import String, UTF8String

fn fib(x: i32) -> i32 {
    if x == 0 {
        return 0
    } else if x == 1 {
        return 1
    } else {
        return fib(x - 1) + fib(x - 2)
    }
}

let x: fn(y: i32) -> i32 = fib

fn main() {
    println("fib 10: "+fib(10)+ ", fib: 32: "+x(32))

    return
}