type Data = class {
    let x: u32
    let y: u32
    let arr: u32[]

    fn init(x: u32, y: u32) {
        this.x = x
        this.y = y

        this.arr = [x, y, 5]
    }

    fn getX() -> u32 = this.x
    fn getY() -> u32 = this.y
    fn getArr() -> u32[] = this.arr
}

fn main() {
    let data = new Data(1, 2)

    return data.getArr()[2]
}
