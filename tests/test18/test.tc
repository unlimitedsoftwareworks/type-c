type Z = class {
    
    let const a: {x: {y: u32}[]}

    fn init(x: u32) {
        this.a.x[0].y = x
        let u = this.a.x
        u[0].y = 1
    }

    fn kill () {
        let u = mutate this.a.x
        u[0].y = 1
    }
}

