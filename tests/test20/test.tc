fn closureTest<T2>(y: T2) -> fn(y:T2) -> T2{
    let z: T2 = 10
     fn f2(x: T2) {
        return z + x
    }

    if y == 0 {
        return f2
    }
    else {
        return closureTest<T2>(y-1)
    }
}

type T = class {
    fn init(x: u32) {
    }
}

let y = closureTest(20)(10)



